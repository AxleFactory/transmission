{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "react-native"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "es6": true
  },
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "react-native/no-unused-styles": 2,
    "react-native/split-platform-components": 2,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "object-curly-spacing": 0,
    "new-cap": 0,
    "space-before-function-paren": ["error", "always"]
  }
}
